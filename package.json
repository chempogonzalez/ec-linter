{
  "name": "@chempo/ec-linter",
  "version": "1.0.9",
  "description": "⚡️ Eslint easy configuration setup. Optimized based on 'standard' config with some opinionated configs. Non extra steps needed",
  "main": "index.js",
  "scripts": {
    "postinstall": "node ./scripts/add-configs-needed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chempogonzalez/ec-linter.git"
  },
  "keywords": [
    "eslint",
    "linter",
    "easy",
    "no config",
    "standard",
    "react",
    "nextjs",
    "typescript"
  ],
  "author": "Chempo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chempogonzalez/ec-linter/issues"
  },
  "homepage": "https://github.com/chempogonzalez/ec-linter#readme",
  "dependencies": {
    "@types/eslint": "8.4.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "deepmerge": "4.2.2",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.28.0",
    "execa": "^5.1.1",
    "fs-extra": "10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "./configs/without-next/.eslintrc.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.18"
  },
  "peerDependencies": {
    "next": ">=10.2.0",
    "typescript": ">=3.3.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  }
}
